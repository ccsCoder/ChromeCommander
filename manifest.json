{
  "manifest_version": 2,

  "name": "Chrome Toolbox",
  "description": "This extension demonstrates Chrome Actions",
  "version": "1.0",

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Chrome Toolbox Actions",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  }
}
